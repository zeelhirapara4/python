   "source": [
    "#11 No is prime or not\n",
    "\n",
    "n=int(input(\"Enter no : \"))\n",
    "c=0\n",
    "if n>1:\n",
    "    for i in range(1,n+1):\n",
    "        if n%i==0:\n",
    "            c+=1\n",
    "    if c==2:\n",
    "        print(\"No is prime\")\n",
    "    else :\n",
    "        print(\"No is not prime\")\n",
    "else :\n",
    "    print(\"No is not prime\")"
   ]
